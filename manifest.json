{
    "manifest_version": 3,
    "name": "ChatGPT Seeker",
    "version": "1.0.0",
    "description": "",
    "permissions": [
        "activeTab",
        "cookies",
        "declarativeContent",
        "scripting",
        "storage",
        "tabs"
    ],
    "host_permissions": [
        "https://*/*"
    ],
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "src/background/background.ts"
    },
    "content_scripts": [
        {
            "matches": [
                "https://*/*"
            ],
            "js": [
                "src/contentScript/main.tsx"
            ],
            "run_at": "document_end"
        }
    ]
}